{
  "name": "zhipuai-sdk-nodejs-v4",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./index.js",
  "types": "type/index.d.ts",
  "type": "module",
  "exports": {
    "types": {
      "require": "./type/index.d.ts",
      "default": "./type/index.d.ts"
    },
    "default": {
      "require": "./dist/cjs/index.cjs",
      "default": "./index.js"
    }
  },
  "scripts": {
    "build": "yarn build:declaration && yarn build:ems && yarn build:cjs",
    "build:ems": "esbuild lib/* --sourcemap --outdir=./dist/ems/",
    "build:cjs": "yarn build:ems --outdir=./dist/cjs/ --format=cjs",
    "build:declaration": "tsc --emitDeclarationOnly --declaration --declarationDir ./type"
  },
  "repository": {
    "url": "https://github.com/winily/zhipuai-sdk-nodejs-v4"
  },
  "devDependencies": {
    "@gjsify/esbuild-plugin-transform-ext": "^0.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "typescript": "^5.3.3",
    "esbuild": "^0.20.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "jsonwebtoken": "^9.0.2"
  }
}